#!/bin/sh
#
# Board specific sensors init
#------------------------------------------------------------------------------
board_adc start

# SPI4
ms5611 -s -b 4 start
icm20602 -s -b 4 -R 12 start

if param compare -s CUBE_MAG_ENABLE 1
then
	icm20948 -s -b 4 -R 10 -M start
else
	icm20948 -s -b 4 -R 10 start
fi


# SPI1
ms5611 -s -b 1 start
icm20649 -s -b 1 start

